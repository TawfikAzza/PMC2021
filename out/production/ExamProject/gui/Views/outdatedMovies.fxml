<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="310.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controller.OutdatedMoviesController">
   <children>
      <TableView fx:id="moviesTableView" layoutX="21.0" layoutY="116.0" prefHeight="261.0" prefWidth="163.0">
        <columns>
          <TableColumn fx:id="title" prefWidth="75.0" text="Movie" />
            <TableColumn fx:id="lastview" prefWidth="87.0" text="Last seen" />
        </columns>
      </TableView>
      <Label layoutX="64.0" layoutY="32.0" text="Welcome to your movie collection!" />
      <Label layoutX="9.0" layoutY="49.0" prefHeight="27.0" prefWidth="306.0" text="Here is a list of movies that you have not seen for a while " />
      <Label layoutX="9.0" layoutY="76.0" text="and that you probably want to delete." />
      <Button layoutX="239.0" layoutY="156.0" mnemonicParsing="false" onAction="#deleteMovie" text="delete" />
      <Button fx:id="cancelButton" layoutX="229.0" layoutY="351.0" mnemonicParsing="false" onAction="#openMainWindow" text="no thanks" />
      <Button fx:id="nextButton" layoutX="244.0" layoutY="200.0" mnemonicParsing="false" onAction="#next" text="next" />
   </children>
</AnchorPane>
